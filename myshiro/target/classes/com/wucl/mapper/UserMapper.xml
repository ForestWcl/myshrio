<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.wucl.mapper.UserMapper">

	<select id="getUserInfoByUsername" parameterType="java.lang.String" resultType="com.wucl.vo.User" >
		SELECT * FROM USER_INFO WHERE USERNAME=#{username}
	</select>

</mapper>